<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">既有產品資訊</h1>
  </div>
  <!-- /.col-lg-12 -->
</div>
<div class="col-lg-6">
  <div class="panel panel-default">
    <div class="panel-heading">產品列表</div>
    <!-- /.panel-heading -->
    <div class="panel-body">
      <div class="table-responsive">
        <table class="table table-striped table-bordered table-hover">
          <thead>
            <tr>
              <th>#</th>
              <th>Name</th>
              <th>產品路徑(電腦)</th>
              <th>產品路徑(手機)</th>
              <th>評測路徑(電腦)</th>
              <th>評測路徑(手機)</th>
              <th>狀態</th>
            </tr>
          </thead>
          <tbody>
          	<% @fet_products.each do |fet_product|%>
	            <tr>
	              <td><%= fet_product.guid %></td>
	              <td><%= fet_product.name %></td>
	              <td><%= fet_product.product_pc_url %></td>
	              <td><%= fet_product.product_mobi_url%></td>
	              <td><%= fet_product.review_pc_url%></td>
	              <td><%= fet_product.review_mobi_url%></td>
	              <td><%= fet_product.status %></td>
	            </tr>
	          <% end %>
          </tbody>
        </table>
      </div>
      <!-- /.table-responsive -->
    </div>
    <!-- /.panel-body -->
  </div>
  <!-- /.panel -->
</div>